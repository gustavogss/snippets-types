    "const PORT = process.env.PORT || 3333;",
    "",
    "app.get('/', (req, res) => {",
    "  res.send(`Servidor rodando na porta ${PORT}`');",
    "});",
    "",
    "app.listen(PORT, () => {",
    "  console.log(`Servidor iniciado na porta ${PORT}`);",
    "});",
    ""
  ],
  "description": "Node com Express"
}
